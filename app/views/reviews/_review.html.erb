<div id="<%= dom_id(review)%>" data-controller="inline-edit">
    <div data-target="inline-edit.content_review" data-action="click->inline-edit#toggle"><%= review.content_review %></div>
    <small>- <%= time_tag review.created_at, "data-local": "time-ago" %></small>

    <%= render "reviews/new", review: review, class: "d-none" %>
</div>